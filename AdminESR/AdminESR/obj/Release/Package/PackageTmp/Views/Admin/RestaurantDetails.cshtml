<head>
    <title></title>
    <script type="text/javascript" src="../../Content/ProjectFiles/AdminView/Restaurants/ViewRestaurants.js"></script>
</head>

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper" style="min-height:900px;">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Restaurant
            <small id="NumberOfRestaurants"></small>
        </h1>

        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Restaurants</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
        <div class="row">
            <!-- /.col -->
            <div class="col-md-12">
                <div class="box box-primary">
                    <div class="box-header with-border">
                        <h3 class="box-title">List</h3>
                        <div class="box-tools pull-right">
                            <div class="has-feedback" id="searchbar1">
                                <section id="ajax-example1">
                                    <form action="#/" onsubmit="return GetSingleRestaurantByName()">
                                        <input type="text" class="form-control input-sm" placeholder="Search for Restaurant" required name="RestaurantSearch" id="RestaurantSearch">
                                    </form>
                                    <script>
                                        var ajax = new XMLHttpRequest();
                                        ajax.open("GET", "http://admin.eatsleeprepeatesr.com/api/restaurants/GetAllClassesNames", true);
                                        ajax.onload = function () {
                                            var list = JSON.parse(ajax.responseText).map(function (i) { return i.Name; });
                                            new Awesomplete(document.querySelector("#ajax-example1 input"), { list: list });
                                        };
                                        ajax.send();
                                    </script>
                                </section>

                                <span class="glyphicon glyphicon-search form-control-feedback"></span>
                            </div>

                        </div>
                        <!-- /.box-tools -->
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body no-padding">
                        <div class="mailbox-controls">
                            <!-- Check all button -->
                            <a href="#/" class="btn btn-default btn-sm" onclick="RestaurantCheakingCheckboxes1()" data-toggle="tooltip" title="Select All">
                                <input type="checkbox" id="select_all1" />
                            </a>
                            <div class="btn-group">
                                <button type="button" onclick="RestaurantDeleteFunction()" class="btn btn-default btn-sm" data-toggle="tooltip" title="Delete"><i class="fa fa-trash-o"></i></button>
                                <a href="#/" onclick="myFunctionSwapView('first_Home_Page');" class="btn btn-default btn-sm"><i class="fa fa-plus"></i></a>
                            </div>
                            <!-- /.btn-group -->
                            <!--forward and previous-->
                            <div class="pull-right" id="DesiMasalaDiv1">
                                <p id="DesiMasalaRestaurant1"></p>
                                <div class="btn-group">
                                    <button type="button" onclick="RestaurantDetailsOpenFunction(this.value)" id="PrevButtonOfRestaurant1" class="btn btn-default btn-sm"><i class="fa fa-chevron-left"></i></button>
                                    <button type="button" onclick="RestaurantDetailsOpenFunction(this.value)" id="NextButtonOfRestaurant1" class="btn btn-default btn-sm"><i class="fa fa-chevron-right"></i></button>

                                </div>
                                <!-- /.btn-group -->
                            </div>
                            <!--/forward and previous-->

                        </div>
                        <div class="table-responsive mailbox-messages" style="margin-top:25px;">
                            <table class="table table-hover table-striped">
                                <thead>
                                    <tr>
                                        <th></th>
                                        <th>Rest_Name</th>
                                        <th>Rest_Area</th>
                                        <th>Rest_Discount</th> 
                                        <th>Rest_Tags</th>
                                        <th>Rest_Deals</th>
                                        <th>Add_Date</th>
                                        <th>Edit</th>
                                    </tr>
                                </thead>
                                <tbody id="RestaurantDetailsDataBody"></tbody>
                            </table>
                            <!-- /.table -->
                        </div>
                        <!-- /.mail-box-messages -->
                    </div>
                    <!-- /.box-body -->
                    <div class="box-footer no-padding">
                        <div class="mailbox-controls">
                            <!-- Check all button -->
                            <a href="#/" class="btn btn-default btn-sm" onclick="RestaurantCheakingCheckboxes2()" data-toggle="tooltip" title="Select All">
                                <input type="checkbox" id="select_all2" />
                            </a>
                            <div class="btn-group">
                                <button type="button" onclick="RestaurantDeleteFunction()" class="btn btn-default btn-sm" data-toggle="tooltip" title="Delete"><i class="fa fa-trash-o"></i></button>
                                <a href="#/" onclick="myFunctionSwapView('first_Home_Page');" class="btn btn-default btn-sm"><i class="fa fa-plus"></i></a>
                            </div>
                            <!-- /.btn-group -->
                            <!--forward and previous-->
                            <div class="pull-right" id="DesiMasalaDiv2">
                                <p id="DesiMasalaRestaurant2"></p>
                                <div class="btn-group">
                                    <button type="button" onclick="RestaurantDetailsOpenFunction(this.value)" id="PrevButtonOfRestaurant2" class="btn btn-default btn-sm"><i class="fa fa-chevron-left"></i></button>
                                    <button type="button" onclick="RestaurantDetailsOpenFunction(this.value)" id="NextButtonOfRestaurant2" class="btn btn-default btn-sm"><i class="fa fa-chevron-right"></i></button>

                                </div>
                                <!-- /.btn-group -->
                            </div>
                            <!--/forward and previous-->
                            <!-- /.pull-right -->
                        </div>
                    </div>
                </div>
                <!-- /. box -->
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </section>
    <!-- /.content -->
</div>


